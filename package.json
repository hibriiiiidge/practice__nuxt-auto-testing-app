{
  "name": "practice__nuxt-auto-testing-app",
  "version": "1.0.0",
  "description": "My peachy Nuxt.js project",
  "author": "Yasunori Takahashi",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "jest"
  },
  "dependencies": {
    "nuxt": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^25.1.0",
    "babel-preset-vue-app": "^2.0.0",
    "jest": "^25.1.0",
    "lodash.clonedeep": "^4.5.0",
    "vue-jest": "^4.0.0-0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/app/$1",
      "^@@/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/app/$1",
      "^~~/(.*)$": "<rootDir>/$1"
    }
  }
}
